                <item>
                    <name>Tamanho Total das Databases</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>perf_counter[\{$MSSQLINST}:Databases(_Total)\Data File(s) Size (KB)]</key>
                    <delay>3600</delay>
                    <history>15d</history>
                    <trends>90d</trends>
